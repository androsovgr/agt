<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox maxWidth="600.0" minWidth="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.mephi.agt.desktop.view.SelfInfoController">
   <children>
      <AnchorPane fx:id="pane" prefHeight="200.0" prefWidth="200.0" />
      <HBox prefHeight="40.0" prefWidth="200.0" spacing="10.0">
         <children>
            <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancel" text="Отмена" />
            <Button fx:id="saveButton" mnemonicParsing="false" onAction="#save" text="Сохранить" />
            <Button fx:id="editButton" mnemonicParsing="false" onAction="#startEdit" text="Изменить" />
         </children>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </HBox>
   </children>
</VBox>
